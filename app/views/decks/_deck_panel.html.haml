.deck-panel
  .deck-info
    .deck-row-one
      %h2{class: 'deck-title'}= deck_string.title
      %h4{class: 'deck-category'}= deck_string.deck.category.name
    .deck-row-two
      %h4{class: 'deck-description'}= deck_string.description
    .deck-row-three
      %p{class: 'deck-tags'} Tags
  .deck-stats
    %p{class: 'deck-total-cards'} Total Cards
    %p{class: 'deck-question-count'} Total Custom Questions
    %p Rating/Score/Comments
